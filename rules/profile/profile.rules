rule all_profile:
    input: TARGET_PROFILE

rule panphlan_profile:
    input:
        MAP_RESULT_DIR
    output:
        PANPHLAN_DIR + "/result_{clade}_presence_absence.csv"
    shell:
        """
        ~/panphlan/panphlan_profile.py \
        -c {wildcards.clade} \
        -i {input} \
        --o_dna {output} \
        --add_strains
        """
